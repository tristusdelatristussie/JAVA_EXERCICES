import java.util.Scanner;

public class Max2 {

		static public int max2(int x, int y) {
			
			
			if(x > y) {
				return x;
			}else {
				return y;
			}
			
			
			
		}

		

		static public int max3(int x, int y, int z) {
			
			if(x > y) {
				return x;
			}else {
				return y;
			}
			
						
			
		}
		
		public static void main(String [] args) {
			
			Scanner sc = new Scanner(System.in);
			System.out.println("Entrez deux entiers");
			System.out.print("x = ");
			int x = sc.nextInt();
			System.out.print("y = ");
			int y = sc.nextInt();
			System.out.print("y = ");
			int z = sc.nextInt();
			
			static int res = max2(x,y);
			System.out.println("Le maximum de x et y vaut " + max3(x,y,z));
			
		}

	
}
