static public int max3(int a, int b, int c) {
	
	while(a - b > 0  || a -c > 0 ) {
		return a;
		break;
	}
	
	
	
}
		
		public static void main(String [] args) {
			
			Scanner sc = new Scanner(System.in);
			System.out.println("Entrez deux entiers");
			System.out.print("x = ");
			int x = sc.nextInt();
			System.out.print("y = ");
			int y = sc.nextInt();
			System.out.print("y = ");
			int z = sc.nextInt();
			System.out.println("Le maximum de x et y vaut " + max3(x,y,z));
			
		}

	
}
